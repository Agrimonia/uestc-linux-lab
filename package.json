    "@types/clear": "^0.1.0",
    "@types/figlet": "^1.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/lowdb": "^1.0.9",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.5",
    "@types/terminal-kit": "^1.28.0",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
